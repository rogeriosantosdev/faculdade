package application;

import java.util.ArrayList;
import java.util.List;

public class Program {
	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		
		list.add("José");
		list.add("Alex");
		list.add("Bob");
		list.add("Ana");
		list.add(2, "Marcos");
		for(String names : list) {
			System.out.println(names);
		}
		
		
		
	}
}
